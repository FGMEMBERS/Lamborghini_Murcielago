<?xml version="1.0"?>

<PropertyList>

 <path>headlights.ac</path>
 <texture-path>Textures</texture-path>


        <animation>
            <type>noshadow</type>
            <object-name>headlights</object-name>
        </animation>


        <animation>
            <type>flash</type>
            <object-name>headlights</object-name>
            <center>
                <x-m>0</x-m>
                <y-m>0</y-m>
                <z-m>-0</z-m>
            </center>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>0</z>
            </axis>
            <power>10</power>
            <factor>1</factor>
            <offset>0</offset>
            <two-sides type="bool">true</two-sides>
            <min>0.7</min>
            <max>1</max>
        </animation>

</PropertyList>
